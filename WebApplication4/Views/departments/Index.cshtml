@model IEnumerable<WebApplication4.department>

@{
    ViewBag.Title = "Index";
}

<h2 style="margin-top:10px;">Department Management</h2>

<p>
    @Html.ActionLink("Add New Department", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.d_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.d_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.d_id }) |
       
            @Html.ActionLink("Delete", "Delete", new { id=item.d_id })
        </td>
    </tr>
}

</table>
